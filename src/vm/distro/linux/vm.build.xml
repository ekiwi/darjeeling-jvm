<project basedir="../../../../">

	<import file="../../vm.build.xml"/>
	<import file="../../vm.generic.build.xml"/>
	
	<!-- the name of this distro -->
	<property name="distro" value="linux"/>
	
	<!-- the cpu and platform -->
	<property name="cpu" value="generic"/>
	<property name="platform" value="linux"/>
	
	<!-- include native code from /opt -->
	<patternset id="opt">
		<include name="darjeeling/linux/**/*.*"/>
	</patternset>
	
</project>
